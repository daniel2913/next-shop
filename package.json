    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "drizzle-orm": "^0.29.0",
