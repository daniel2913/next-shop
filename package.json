{
  "name": "next-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next:dev": "next dev",
    "dev": "start npm run dev-next & start npm run image-dev",
    "image": "deno run --allow-ffi --allow-net --allow-read --allow-write imageserver.ts",
    "image:dev": "deno run --allow-ffi --watch --allow-net --allow-read --allow-write imageserver.ts",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev-turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
