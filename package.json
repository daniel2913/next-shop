{
	"name": "next-shop",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
		"dev-turbo": "next dev --turbo",
		"build": "next build",
		"start": "next start",
		"lint": "npx eslint src",
