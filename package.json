		"format": "npm run prettier:fix && npm run lint:fix"
	},
	"dependencies": {
		"@material-tailwind/react": "^2.1.8",
		"@next/eslint-plugin-next": "^14.0.3",
		"drizzle-orm": "^0.29.0",
		"next": "^14.0.3",
		"next-auth": "^4.24.4",
		"postgres": "^3.4.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-is": "^18.2.0",
		"sharp": "^0.33.2",
		"zod": "^3.22.4",
		"zustand": "^4.3.8"
	},
	"devDependencies": {
		"@svgr/webpack": "^8.0.1",
		"@types/node": "^20.8.10",
		"@types/react": "^18.2.34",
		"@types/react-dom": "^18.2.14",
		"autoprefixer": "^10.4.16",
		"cross-env": "^7.0.3",
		"eslint": "^8.55.0",
		"eslint-config-next": "14.0.3",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-react": "^7.33.2",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"tailwindcss": "^3.3.5",
