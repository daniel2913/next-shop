{
	"extends": [
		"next/core-web-vitals",
		"plugin:react/recommended",
		"eslint:recommended",
		"prettier"
	],
	"env":{
		"browser":true,
		"node":true,
		"es2020":true
	},
	"rules": {
		"react/react-in-jsx-scope": "off",
		"no-unused-vars": "off",
		"complexity":"warn",
		"eqeqeq":"warn",
		"prefer-template":"warn"
	}
}
